{
  "name": "vfm",
  "private": true,
  "version": "4.4.5",
  "scripts": {
    "dev": "concurrently \"npm run:dev:docs\" \"npm run:dev:viteplay\"",
    "dev:docs": "npm run --filter docs dev",
    "build:docs": "npm run --filter docs build",
    "generate:docs": "npm run --filter docs generate",
    "dev:viteplay": "npm run --filter viteplay dev",
    "build:viteplay": "npm run --filter viteplay build",
    "dev:vfm": "npm run --filter vue-final-modal dev",
    "build:vfm": "npm run --filter vue-final-modal build",
    "test:vfm": "npm run --filter vue-final-modal cypress:run",
    "release:vfm": "npm run --filter vue-final-modal release",
    "lint": "eslint . --ext=.ts,.vue --fix",
    "typecheck": "npm run --parallel typecheck",
  },
  "dependencies": {
    "vue": "3.4.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.7",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "pnpm": "^7.26.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vue-tsc": "^1.0.24"
  }
}
